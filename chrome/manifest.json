{
  "manifest_version": 2,

  "name": "GooberGabber",
  "description": "Replaces GamerGate with other G words. Also replaces GamerGater with Alligator.",
  "version": "1.0",
  "author": "SpiltCoffee",

  "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["goobergabber.js"],
      "run_at": "document_end"
    }
  ]
}